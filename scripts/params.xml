<?xml version="1.0"?>
<parameters>
  <!-- Time related parameters -->
  <param max="2000." min="0.1" name="span" step="10." value="900.0">
    <unit>s</unit>
    <description>Simulation duration</description>
  </param>
  <param max="2." min="0.1" name="dt" step="0.1" value="0.5">
    <unit>s</unit>
    <description>Time step</description>
  </param>
  <param max="1000." min="0." name="t_A" step="1." value="720">
    <unit>s</unit>
    <description>Anaphase A transition</description>
  </param>
  <!-- attachment instability -->
  <param max="100" min="0" name="d_alpha" step="0.005" value="0.05">
    <unit>&#181;m</unit>
    <description>Aurora Pdet modifier</description>
  </param>
  <param max="100" min="0" name="orientation" step="1" value="1">
    <unit> </unit>
    <description>Orientation effect </description>
  </param>
  <param max="1" min="0" name="sac" step="1" value="1">
    <unit> </unit>
    <description>Non zero: spindle assembly checkpt </description>
  </param>
  <param max="1." min="0." name="k_a" step="0.001" value="0.06">
    <unit>Hz</unit>
    <description>MT free attachement frequency</description>
  </param>
  <!-- Spindle -->
  <param max="2000." min="1." name="mus" step="1." value="500.0">
    <unit>pN.s/&#181;m</unit>
    <description>Spb drag coefficient</description>
  </param>
  <param max="15." min="0." name="L0" step="0.1" value="0.3">
    <unit>&#181;m</unit>
    <description>Spindle initial length</description>
  </param>
  <!-- Number of elements -->
  <param max="23" min="0" name="N" step="1" value="3">
    <unit> </unit>
    <description>Number of chromosomes</description>
  </param>
  <param max="100" min="0" name="Mk" step="1" value="4">
    <unit> </unit>
    <description>Number of attachment sites</description>
  </param>
<!-- Length dependance -->
<param max="2." min="0." name="ldep" step="0" value="1">
  <unit> </unit>
  <description>Fk length dependance coefficient</description>
</param>

<!--  Non free parameters - changing values below this line is useless -->
<param max="1." min="0." name="k_d0" step="0.001" value="0.06">
  <unit>Hz</unit>
  <description>MT free detachement frequency</description>
</param>
<param max="200." min="1." name="kappa_c" step="0" value="40.0">
  <unit>pN/&#181;m</unit>
  <description>Cohesin spring constant</description>
</param>
<param max="200." min="1." name="kappa_k" step="0" value="50.0">
  <unit>pN/&#181;m</unit>
  <description>Chromatid spring  constant</description>
</param>
<param max="2000." min="0.1" name="muc" step="0" value="400.0">
  <unit>pN.s/&#181;m</unit>
  <description>Chromatid drag coefficient</description>
</param>
<param max="2000." min="1." name="muk" step="0" value="500.0">
  <unit>pN.s/&#181;m</unit>
  <description>Kt structural friction coef.</description>
</param>
<param max="1." min="0.01" name="d0" step="0" value="0.05">
  <unit>&#181;m</unit>
  <description>Kinetochore - kinetochore rest length</description>
</param>
<param max="0.1" min="0.005" name="Vk" step="0" value="0.03">
  <unit>&#181;m/s</unit>
  <description>Kineto poleward mvt max. speed</description>
</param>
<param max="0.1" min="0.005" name="Vmz" step="0" value="0.03">
  <unit>&#181;m/s</unit>
  <description>Midzone force max. speed</description>
</param>
<param max="100." min="0.1" name="Fk" step="0" value="10.0">
  <unit>pN</unit>
  <description>Kineto polward mvmt stall force</description>
</param>
<param max="1000." min="1." name="Fmz" step="0" value="100.0">
  <unit>pN</unit>
  <description>Midzone stall Force</description>
</param>
<param max="2." min="0." name="k_spb" step="0.1" value="1">
  <unit>&#181;m</unit>
  <description>Mean kt-spb distance</description>
</param>
<param max="2." min="0." name="ldep_balance" step="0" value="1">
  <unit> </unit>
  <description>Length dependance balance</description>
</param>
</parameters>
